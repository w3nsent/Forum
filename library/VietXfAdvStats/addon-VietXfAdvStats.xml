    <template title="VietXfAdvStats.css"><![CDATA[.xenForm .ctrlUnit dd input.VietXfAdvStats_OptionSectionsTitle,
.xenForm .ctrlUnit dd select.VietXfAdvStats_OptionSectionsSelect,
.xenForm .ctrlUnit dd span.VietXfAdvStats_OptionSections_ForumSelect select {
	width: 150px;
}]]></template>
    <template title="VietXfAdvStats_option_numbers"><![CDATA[<xen:require js="js/VietXfAdvStats/backend.js" />

<xen:controlunit label="{$preparedOption.title}" hint="{$preparedOption.hint}">
	<xen:explain>{xen:raw $preparedOption.explain}</xen:explain>
	<xen:html>
		<ul>
			<xen:foreach loop="$numbers" key="$counter" value="$number">
			<li>
				<xen:textbox 
					name="{$fieldPrefix}[{$preparedOption.option_id}][{$counter}]"
					value="{$number}"
					/>
			</li>
			</xen:foreach>
		
			<li class="VietXfAdvStats_OptionNumbersListener">
				<xen:textbox 
					name="{$fieldPrefix}[{$preparedOption.option_id}][{$nextCounter}]"
					value=""
					/>
			</li>
		</ul>
		
		<input type="hidden" name="{$listedFieldName}" value="{$preparedOption.option_id}" />
		{xen:raw $editLink}
	</xen:html>
</xen:controlunit>]]></template>
    <template title="VietXfAdvStats_option_sections"><![CDATA[<xen:controlunit label="{$preparedOption.title}" hint="{$preparedOption.hint}">
	<xen:explain>{xen:raw $preparedOption.explain}</xen:explain>
	<xen:html>
		<ul>
			<xen:if is="{$configured}">
				<xen:foreach loop="$configured" key="$counter" value="$section">
				<li>
					<xen:include template="VietXfAdvStats_option_sections_single" />
				</li>
				</xen:foreach>
			</xen:if>
			<li class="VietXfAdvStats_OptionSectionsListener">
				<div class="VietXfAdvStats_OptionSectionsSingle">
					<xen:textbox 
						name="{$fieldPrefix}[{$preparedOption.option_id}][{$nextCounter}][title]"
						placeholder="{xen:phrase VietXfAdvStats_section_title} ({xen:phrase optional})"
						inputclass="VietXfAdvStats_OptionSectionsTitle"
						/>
					<xen:select name="{$fieldPrefix}[{$preparedOption.option_id}][{$nextCounter}][type]" inputclass="VietXfAdvStats_OptionSectionsSelect">
						<xen:option value="">{xen:phrase VietXfAdvStats_not_used}</xen:option>
						<xen:options source="{$availableSections}" />
					</xen:select>
					<span class="VietXfAdvStats_OptionSections_ForumSelect" style="width:100%">
						<xen:select name="{$fieldPrefix}[{$preparedOption.option_id}][{$nextCounter}][forum_id]" multiple="true">
							<xen:options source="{$forums}" />
						</xen:select>
					</span>
				</div>
			</li>
		</ul>
		
		<input type="hidden" name="{$listedFieldName}" value="{$preparedOption.option_id}" />
		{xen:raw $editLink}
	</xen:html>
</xen:controlunit>]]></template>
    <template title="VietXfAdvStats_option_sections_single"><![CDATA[<xen:require js="js/VietXfAdvStats/backend.js" />
<xen:require css="VietXfAdvStats.css" />

<div class="VietXfAdvStats_OptionSectionsSingle">
	<xen:textbox 
		name="{$fieldPrefix}[{$preparedOption.option_id}][{$counter}][title]"
		value="{xen:if '{$section.title}','{$section.title}'}"
		placeholder="{xen:phrase VietXfAdvStats_section_title} ({xen:phrase optional})"
		inputclass="VietXfAdvStats_OptionSectionsTitle"
		/>
	<xen:select name="{$fieldPrefix}[{$preparedOption.option_id}][{$counter}][type]" value="{xen:if '{$section.type}','{$section.type}'}" inputclass="VietXfAdvStats_OptionSectionsSelect">
		<xen:option value="">{xen:phrase VietXfAdvStats_not_used}</xen:option>
		<xen:options source="{$availableSections}" />
	</xen:select>
	<span class="VietXfAdvStats_OptionSections_ForumSelect" style="width:100%">
	<xen:if is="{$section.forums}">
		<xen:select name="{$fieldPrefix}[{$preparedOption.option_id}][{$counter}][forum_id]" multiple="true">
			<xen:options source="{$section.forums}" />
		</xen:select>
	<xen:else/>
		<xen:select name="{$fieldPrefix}[{$preparedOption.option_id}][{$counter}][forum_id]" value="{xen:if $section.forum_id,$section.forum_id}" multiple="true">
			<xen:options source="{$forums}" />
		</xen:select>
	</xen:if>
	</span>
</div>]]></template>
